    "def printpattern(num):\n",
    "    for i in range(1,num):\n",
    "        for j in range(1,i+1):\n",
    "            pass\n",
    "\n",
    "        if j > (num/2):\n",
    "            print('*'*(num-j))\n",
    "        else:\n",
    "            print('*'*j)\n",
    "\n",
    "printpattern(10)  "
   ]
